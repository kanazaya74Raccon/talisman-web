{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@root": ["./"],
      "@root/*": ["./*"],
      "@archetypes": ["./archetypes"],
      "@archetypes/*": ["./archetypes/*"],
      "@assets": ["./assets"],
      "@assets/*": ["./assets/*"],
      "@components": ["./components"],
      "@components/*": ["./components/*"],
      "@icons": ["./assets/icons"],
      "@icons/*": ["./assets/icons/*"],
      "@libs": ["./libs"],
      "@libs/*": ["./libs/*"],
      "@routes": ["./routes"],
      "@routes/*": ["./routes/*"],
      "@util": ["./util"],
      "@util/*": ["./util/*"]
    }
  }
}
